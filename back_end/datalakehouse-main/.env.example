# Data Lakehouse Configuration Template
# Copy this file to .env and customize for your environment.
# NEVER commit .env to version control - it contains secrets!

# ====================================
# MinIO (S3-compatible Object Storage)
# ====================================
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=changeme_in_production
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=changeme_in_production
MINIO_BUCKET_UPLOADS=uploads
MINIO_BUCKET_WAREHOUSE=warehouse
MINIO_PORT_API=9000
MINIO_PORT_CONSOLE=9001

# ====================================
# RabbitMQ (Message Queue)
# ====================================
RABBITMQ_DEFAULT_USER=admin
RABBITMQ_DEFAULT_PASS=changeme_in_production
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_QUEUE_NAME=file.processing.queue

# ====================================
# PostgreSQL (Iceberg Catalog)
# ====================================
POSTGRES_USER=iceberg_user
POSTGRES_PASSWORD=changeme_in_production
POSTGRES_DB=iceberg_catalog
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# ====================================
# Redis (Job Status Cache)
# ====================================
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PORT_EXTERNAL=6380

# ====================================
# API Service (Spring Boot)
# ====================================
API_SERVICE_PORT=8080
SPRING_PROFILES_ACTIVE=prod

# ====================================
# Spark Worker
# ====================================
API_BASE_URL=http://api-service:8080

# ====================================
# File Upload Settings
# ====================================
FILE_MAX_SIZE=104857600
FILE_ALLOWED_TYPES=xlsx,xls,csv,parquet,avro

# ====================================
# Iceberg Warehouse
# ====================================
ICEBERG_WAREHOUSE_PATH=s3a://warehouse/wh/
ICEBERG_CATALOG_TYPE=jdbc
ICEBERG_CATALOG_URI=jdbc:postgresql://postgres:5432/iceberg_catalog

# ====================================
# AWS/S3 Settings (for MinIO)
# ====================================
AWS_REGION=us-east-1
S3_PATH_STYLE_ACCESS=true
